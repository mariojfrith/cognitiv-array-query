{
  "name": "cognitiv-array-query",
  "private": true,
  "descrption": "A Javascript class to execute MongoDb-like queries on arrays.",
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "compile": "babel index.js -o index.compiled.js",
    "minify": "uglifyjs index.compiled.js -o index.min.js",
    "build": "webpack --config webpack.config.js",
    "all": "npm run test && npm run build && npm run test"
  },
  "directories": {
    "test": "**/__tests__"
  },
  "dependencies": {
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "terser-webpack-plugin": "^5.3.10",
    "uglify-js": "^3.19.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": ["node_modules", "src"],
    "testMatch": ["**/__tests__/**/*.test.js"],
    "verbose": true
  },
  "keywords": [
    "MongoDB",
    "query",
    "javascript",
    "array",
    "filtering",
    "CognitivTraits"
  ],
  "author": "Mario"
}
